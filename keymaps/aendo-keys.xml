<keymap version="1" name="aendo-keys" parent="$default">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt left" />
  </action>
  <action id="Diff.NextChange" />
  <action id="Diff.PrevChange" />
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="ImplementMethods" />
  <action id="NextTab" />
  <action id="PreviousTab" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl i" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl r" />
  </action>
  <action id="Replace" />
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>